{
  "name": "reactjs-faunadb",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "scripts": {
    "clean": "rm -rf out functions-build",
    "ðŸ‘‡ ABOUT-nextjs-command": "ðŸ’¡ NextJS commands #",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:export": "next export",
    "ðŸ‘‡ ABOUT-faunadb-command": "ðŸ’¡ scaffold and setup FaunaDB #",
    "db:bootstrap": "export FAUNADB_SERVER_SECRET=$(cat '.key') && node ./scripts/bootstrap-fauna-database.js",
    "ðŸ‘‡ ABOUT-start-command": "ðŸ’¡ start the app and server #",
    "start": "npm-run-all --parallel start:app start:functions",
    "start:app": "npm run next:dev",
    "start:functions": "npm run netlify:functions:serve",
    "ðŸ‘‡ ABOUT-netlify-command": "ðŸ’¡ Netifly commands #",
    "netlify:functions:serve": "export FAUNADB_SERVER_SECRET=$(cat '.key') && netlify-lambda serve functions -c ./webpack.config.js",
    "netlify:functions:build": "export FAUNADB_SERVER_SECRET=$(cat '.key') && netlify-lambda build functions -c ./webpack.config.js",
    "netlify:deploy": "npm run next:build"
  },
  "dependencies": {
    "next": "9.4.1",
    "react": "16.13.1",
